{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@jmtf/shared": [
        "libs/shared/src"
      ],
      "@jmtf/shared/*": [
        "libs/shared/src/*"
      ],
      "@app/shareds": [
        "libs/shareds/src"
      ],
      "@app/shareds/*": [
        "libs/shareds/src/*"
      ]
    }
  }
}